{
	"name": "fuma",
	"version": "2.0.2",
	"description": "My fullstack material build with sveltekit, daisyui, zod, prisma, lucia",
	"author": {
		"name": "Jonas Voisard",
		"email": "jonas.voisard@gmail.com"
	},
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build && npm run package",
		"preview": "vite preview --host",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"migrate": "prisma migrate dev",
		"generate": "prisma generate",
		"studio": "prisma studio",
		"seed": "prisma db seed"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"prisma": {
		"seed": "node --loader ts-node/esm prisma/seed.ts"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./ui": {
			"types": "./dist/ui/index.d.ts",
			"svelte": "./dist/ui/index.js",
			"default": "./dist/ui/index.js"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"default": "./dist/utils/index.js"
		},
		"./api": {
			"types": "./dist/api/index.d.ts",
			"default": "./dist/api/index.js"
		},
		"./store": {
			"types": "./dist/store/index.d.ts",
			"default": "./dist/store/index.js"
		},
		"./validation": {
			"types": "./dist/validation/index.d.ts",
			"default": "./dist/validation/index.js"
		},
		"./action": {
			"types": "./dist/action/index.d.ts",
			"default": "./dist/action/index.js"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"default": "./dist/server/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.3.1",
		"@sveltejs/kit": "^2.43.5",
		"@sveltejs/package": "^2.5.4",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/typography": "^0.5.19",
		"@types/debounce": "^1.2.4",
		"@types/eslint": "^8.56.12",
		"@types/node": "^24.5.2",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"autoprefixer": "^10.4.21",
		"daisyui": "5.0.0-beta.3",
		"eslint": "^8.57.1",
		"eslint-config-prettier": "^9.1.2",
		"eslint-plugin-svelte": "^2.46.1",
		"postcss": "^8.5.6",
		"postcss-load-config": "^5.1.0",
		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"prettier-plugin-tailwindcss": "^0.6.14",
		"prisma": "^5.22.0",
		"publint": "^0.1.16",
		"svelte-check": "^3.8.6",
		"tailwindcss": "^4.1.13",
		"tslib": "^2.8.1",
		"typescript": "^5.9.2",
		"vitest": "^1.6.1"
	},
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^4.0.1",
		"@mdi/js": "^7.4.47",
		"@prisma/client": "5.22.0",
		"@sveltejs/vite-plugin-svelte": "5.0.1",
		"@tailwindcss/vite": "^4.1.13",
		"@tiptap/core": "^2.26.2",
		"@tiptap/extension-color": "^2.26.2",
		"@tiptap/extension-highlight": "^2.26.2",
		"@tiptap/extension-image": "^2.26.2",
		"@tiptap/extension-link": "^2.26.2",
		"@tiptap/extension-mention": "^2.26.2",
		"@tiptap/extension-placeholder": "^2.26.2",
		"@tiptap/extension-text-align": "^2.26.2",
		"@tiptap/extension-text-style": "^2.26.2",
		"@tiptap/extension-youtube": "^2.26.2",
		"@tiptap/html": "^2.26.2",
		"@tiptap/pm": "^2.26.2",
		"@tiptap/starter-kit": "^2.26.1",
		"@tiptap/suggestion": "^2.26.2",
		"axios": "^1.12.2",
		"dayjs": "^1.11.18",
		"debounce": "^2.2.0",
		"devalue": "^4.3.3",
		"litepicker": "^2.0.12",
		"lucia": "^3.2.2",
		"mode-watcher": "^0.5.1",
		"oslo": "^1.2.1",
		"svelte": "^5.39.6",
		"svelte-easy-crop": "^3.0.1",
		"svelte-sonner": "^0.3.28",
		"tippy.js": "^6.3.7",
		"ts-node": "^10.9.2",
		"vite": "^7.1.7",
		"zod": "^4.1.11"
	}
}
