{
	"name": "fuma",
	"version": "0.3.51",
	"description": "My fullstack material build with sveltekit, daisyui, zod, prisma, lucia",
	"author": {
		"name": "Jonas Voisard",
		"email": "jonas.voisard@gmail.com"
	},
	"scripts": {
		"dev": "vite dev --host",
		"build": "vite build && npm run package",
		"preview": "vite preview --host",
		"package": "svelte-kit sync && svelte-package && publint",
		"prepublishOnly": "npm run package",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"test": "vitest",
		"lint": "prettier --check . && eslint .",
		"format": "prettier --write .",
		"migrate": "prisma migrate dev",
		"generate": "prisma generate",
		"studio": "prisma studio",
		"seed": "prisma db seed"
	},
	"svelte": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"type": "module",
	"prisma": {
		"seed": "node --loader ts-node/esm prisma/seed.ts"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		},
		"./ui": {
			"types": "./dist/ui/index.d.ts",
			"svelte": "./dist/ui/index.js",
			"default": "./dist/ui/index.js"
		},
		"./utils": {
			"types": "./dist/utils/index.d.ts",
			"default": "./dist/utils/index.js"
		},
		"./api": {
			"types": "./dist/api/index.d.ts",
			"default": "./dist/api/index.js"
		},
		"./store": {
			"types": "./dist/store/index.d.ts",
			"default": "./dist/store/index.js"
		},
		"./validation": {
			"types": "./dist/validation/index.d.ts",
			"default": "./dist/validation/index.js"
		},
		"./action": {
			"types": "./dist/action/index.d.ts",
			"default": "./dist/action/index.js"
		},
		"./server": {
			"types": "./dist/server/index.d.ts",
			"default": "./dist/server/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/**/*.test.*",
		"!dist/**/*.spec.*"
	],
	"peerDependencies": {
		"@sveltejs/kit": "^2.5.6",
		"svelte": "^4.0.0"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^3.0.0",
		"@sveltejs/kit": "^2.5.7",
		"@sveltejs/package": "^2.3.1",
		"@sveltejs/vite-plugin-svelte": "^3.1.0",
		"@tailwindcss/typography": "^0.5.13",
		"@types/debounce": "^1.2.4",
		"@types/eslint": "^8.56.0",
		"@types/node": "^20.12.7",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"autoprefixer": "^10.4.16",
		"daisyui": "^4.10.2",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-svelte": "^2.35.1",
		"postcss": "^8.4.32",
		"postcss-load-config": "^5.0.2",
		"prettier": "^3.1.1",
		"prettier-plugin-svelte": "^3.1.2",
		"prettier-plugin-tailwindcss": "^0.5.9",
		"prisma": "^5.13.0",
		"publint": "^0.1.9",
		"svelte": "^4.2.15",
		"svelte-check": "^3.6.0",
		"tailwindcss": "^3.3.6",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^5.2.11",
		"vitest": "^1.2.0"
	},
	"dependencies": {
		"@lucia-auth/adapter-prisma": "^4.0.1",
		"@mdi/js": "^7.4.47",
		"@prisma/client": "5.13.0",
		"@tiptap/core": "^2.3.0",
		"@tiptap/extension-color": "^2.3.0",
		"@tiptap/extension-highlight": "^2.3.0",
		"@tiptap/extension-image": "^2.3.0",
		"@tiptap/extension-link": "^2.3.0",
		"@tiptap/extension-mention": "^2.3.0",
		"@tiptap/extension-placeholder": "^2.3.0",
		"@tiptap/extension-text-align": "^2.3.0",
		"@tiptap/extension-text-style": "^2.3.0",
		"@tiptap/extension-youtube": "^2.3.0",
		"@tiptap/html": "^2.3.0",
		"@tiptap/pm": "^2.3.0",
		"@tiptap/starter-kit": "^2.3.0",
		"@tiptap/suggestion": "^2.3.0",
		"axios": "^1.6.8",
		"dayjs": "^1.11.10",
		"debounce": "^2.0.0",
		"devalue": "^4.3.2",
		"litepicker": "^2.0.12",
		"lucia": "^3.1.1",
		"mode-watcher": "^0.3.0",
		"oslo": "^1.2.0",
		"svelte-easy-crop": "^2.0.3",
		"svelte-sonner": "^0.3.22",
		"tippy.js": "^6.3.7",
		"ts-node": "^10.9.2",
		"zod": "^3.22.4"
	}
}
